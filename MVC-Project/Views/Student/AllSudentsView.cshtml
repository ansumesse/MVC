<table class="table table-bordered">
	@foreach (Student student in Model)
	{
		<tr>
			<td>@student.Id</td>
			<td>@student.Name</td>
			<td>@student.Address</td>
			<td><img src="/Images/@student.Image" style="width:100px"/></td>
			<td><a href="/Student/Details/@student.Id">Details</a></td>
		</tr>
	}
</table>
